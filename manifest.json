{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "0.0.1",
    "description": "This extension is a sample extension",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "extension_style.css"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Search on Cambridge"
    }
}